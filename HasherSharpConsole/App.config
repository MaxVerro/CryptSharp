<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section
        name="hasherSharpConfig"
        type="HasherSharp.Configurations.HasherSharpConfig, HasherSharp" 
      />
    <!-- Other <section> and <sectionGroup> elements. -->
  </configSections>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <hasherSharpConfig 
      algorithm="HMACSHA1"
      saltBytesLength="32"
      hashBytesLength="20"
      iterations="92000"
    />

</configuration>